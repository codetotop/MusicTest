<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FormInputView">
        <attr name="tvlabel_name" format="string" />
        <attr name="edtvalue_inputtype" format="integer|string|reference" />
        <attr name="imgevent_source" format="reference" />
        <attr name="imgevent_action" format="string" />
        <attr name="imgevent_visibility" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DocumentView">
        <attr name="imgavatardocument_backgound" format="reference|color" />
        <attr name="tvnamedocument_name" format="string" />
        <attr name="imgdownloaddocument_visible" format="boolean" />
        <attr name="imgdownloaddocument_backgound" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ProfileView">
        <attr name="profileview_avatar" format="reference|color" />
        <attr name="profileview_name" format="reference|string" />
        <attr name="profileview_duty" format="reference|string" />
        <attr name="profileview_department" format="reference|string" />
        <attr name="profileview_action_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ProfileInformationView">
        <attr name="profileinformationview_title" format="string|reference" />
        <attr name="profileinformationview_content" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="ProfileInputView">
        <attr name="profileinputview_title" format="string|reference" />
        <attr name="profileinputview_content_hint" format="string|reference" />
        <attr name="profileinputview_content_focusable" format="boolean" />
        <attr name="profileinputview_content_drawable_right" format="reference|color" />
        <attr name="profileinputview_content_drawable_right_show" format="boolean" />
        <attr name="profileinputview_content" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="ToolbarView" />
</resources>